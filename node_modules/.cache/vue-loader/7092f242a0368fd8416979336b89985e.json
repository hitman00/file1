n      }\n    },\n    mounted() {\n      this.roll = document.getElementById('scroll_t').offsetTop;\n    },\n    components: {\n      PlayVideo,\n      ImageDash\n    }\n  };\n</script>\n\n<style lang=\"scss\" module>\n  .box {\n    background: #f9ebea;\n    h1 {\n      font-size: 16px;\n      color: #000;\n      font-weight: bold;\n      text-align: center;\n      word-spacing: -1px;\n      padding-top: 29px;\n      padding-bottom: 10px;\n      line-height: 25px;\n    }\n    p {\n      margin: 9px 31px 20px;\n      font-size: 14px;\n      line-height: 1.7;\n      text-align: justify;\n      text-align-last: center;\n      color: #341d51;\n      word-spacing: -3px;\n    }\n  }\n  .botton {\n    background: #bc96ff;\n    border-radius: 5px;\n    margin: 0 36.4% 39px;\n    font-size: 15px;\n    color: #fff;\n    padding: 5px;\n    text-align: center;\n    cursor: pointer;\n    line-height: 24px;\n    transition: all 0.5s;\n    &:hover {\n      opacity: 0.8;\n    }\n  }\n</style>\n"]}]}